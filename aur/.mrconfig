[DEFAULT]
include = cat "$AB2_ARCH_DIR/mrcommon"

fetch = 
	check_package_directory "$PWD"
	aur_update "$PWD"

register = 
	check_package_directory "$PWD"
	pkgname=$( basename "$PWD" )
	mr config "$pkgname" checkout="aur_checkout $pkgname"

checkout = aur_checkout "$MR_REPO"


[acroread]

[dropbox]

[ec2-api-tools]

[impose+]

[python2-pyicu]

[python2-vobject]

[coffee-script]

[perl-term-extendedcolor]

[underscorejs]

[nodejs-mkdirp]

[lib32-pangox-compat]
